	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.2",
		"express-async-handler": "^1.2.0",
		"mongoose": "^6.1.5"
	},
	"devDependencies": {
		"concurrently": "^7.0.0",
